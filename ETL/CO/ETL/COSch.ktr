        &quot;BUSINESS_UNIT_K&quot; 
    );

CREATE TABLE &quot;ERPI&quot;.&quot;COST_CENTER&quot;
(
    &quot;COST_CENTER_K&quot; 	INTEGER NOT NULL,
    &quot;COST_CENTER_NK&quot; 	VARCHAR(40) NOT NULL DEFAULT &apos;Unspecified&apos;,
  	&quot;NAME&quot;		 		character varying(128) NOT NULL DEFAULT &apos;Unspecified&apos;,
    &quot;DATE_FROM&quot; 		TIMESTAMP,
    &quot;DATE_TO&quot; 			TIMESTAMP,
    &quot;VERSION&quot; 			INTEGER
);

