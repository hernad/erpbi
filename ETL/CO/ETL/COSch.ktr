    &quot;BUSINESS_UNIT&quot; 	VARCHAR(128) NOT NULL DEFAULT &apos;Unspecified&apos;,
    &quot;DATE_FROM&quot; 	TIMESTAMP,
    &quot;DATE_TO&quot; 		TIMESTAMP,
